{
  "buildCommand": "npm run build",
  "outputDirectory": "./dist/public",
  "framework": "vite",
  "installCommand": "npm ci",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}